{
  "name": "aws-infra",
  "version": "1.0.0",
  "scripts": {
    "aws-cdk": "env-cmd --use-shell -- \"npx cdk ls\"",
    "aws-bootstrap": "env-cmd --use-shell -- \"npx cdk bootstrap\"",
    "aws-deploy": "env-cmd --use-shell -- \"npx cdk deploy\"",
    "aws-deploy-nr": "env-cmd --use-shell -- \"npx cdk deploy --no-rollback\"",
    "aws-deploy-ext-service": "env-cmd --use-shell -- \"npx cdk deploy ExtServiceStack --no-rollback\"",
    "aws-deploy-worker": "env-cmd --use-shell -- \"npx cdk deploy WorkerStack --no-rollback\"",
    "aws-destroy": "env-cmd --use-shell -- \"npx cdk destroy\"",
    "aws-synth": "env-cmd --use-shell -- \"npx cdk synth\""
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "dotenv-flow": "^4.1.0",
    "tsx": "^4.19.4"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.3.3",
    "@types/node": "^22.15.3",
    "aws-cdk": "^2.1013.0",
    "aws-cdk-lib": "^2.194.0",
    "constructs": "^10.4.2",
    "env-cmd": "^10.1.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@8.10.5",
  "engines": {
    "node": ">=23.0.0"
  },
  "pnpm": {
    "neverBuiltDependencies": []
  }
}